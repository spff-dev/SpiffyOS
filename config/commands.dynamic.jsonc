{
  // Dynamic commands are routed to named handlers in code
  "prefix": "!",
  "commands": {
    "uptime": { "handler": "Uptime" },
    "title": { "handler": "SetTitle", "modOnly": true },
    "game": { "handler": "SetGame", "modOnly": true },
    "so": { "handler": "Shoutout", "modOnly": true },
    "announce": { "handler": "Announcement", "modOnly": true }
  }
}
